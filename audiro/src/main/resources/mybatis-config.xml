        <typeAlias type="com.audiro.repository.Post" alias="Post"/>
        <typeAlias type="com.audiro.repository.DraftPost" alias="DraftPost"/>
        <typeAlias type="com.audiro.repository.PostType" alias="PostType"/>
        <typeAlias type="com.audiro.repository.Comments" alias="Comments"/>
        <typeAlias type="com.audiro.repository.FavoriteUsers" alias="FavoriteUsers"/>
        <typeAlias type="com.audiro.repository.FavoritePost" alias="FavoritePost"/>
